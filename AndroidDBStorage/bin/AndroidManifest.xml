<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xjc.androiddbstorage"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.xjc.androiddbstorage.UserListActivity"
            android:label="用户管理" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.xjc.android.SettingActivity"
            android:label="系统设置" >
        </activity>

        <service android:name="com.xjc.android.MyService" >
        </service>

        <activity android:name="com.xjc.android.ServiceActivity" >
        </activity>
        <activity
            android:name="com.xjc.android.SharedPreferenceActivity"
            android:label="@string/title_activity_shared_preference" >
        </activity>
    </application>

</manifest>
